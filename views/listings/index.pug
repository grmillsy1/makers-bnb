extends ../layout

block content
  h1 Listing form submitted
  each val in listings
    a(href='listings/view?id=' + val.id)
      div(class='listing')
        div(class='listing-title')=val.title
        div(class='listing-description')=val.description
        div(class='listing-location')=val.location
        div(class='listing-price')='Â£' + val.price + 'per night'

    br
      
  form(action="/", method="get")
    div.input
    div.actions
    input(type="submit", value="Go Home")
